<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/santri_nav"
    app:startDestination="@id/listFragmentAsatid">

    <fragment
        android:id="@+id/listFragment"
        android:name="id.ac.poltektegal.uasmpdanoop2.fragments.list.ListFragment"
        android:label="List"
        tools:layout="@layout/fragment_list" >
        <action
            android:id="@+id/action_listFragment_to_addFragment"
            app:destination="@id/addFragment" />
        <action
            android:id="@+id/action_listFragment_to_updateFragment"
            app:destination="@id/updateFragment" />
        <action
            android:id="@+id/action_listFragment_to_home2"
            app:destination="@id/home2" />
    </fragment>


    <fragment
        android:id="@+id/addFragment"
        android:name="id.ac.poltektegal.uasmpdanoop2.fragments.add.AddFragment"
        android:label="Add"
        tools:layout="@layout/fragment_add" >
        <action
            android:id="@+id/action_addFragment_to_listFragment"
            app:destination="@id/listFragment" />
    </fragment>


    <fragment
        android:id="@+id/updateFragment"
        android:name="id.ac.poltektegal.uasmpdanoop2.fragments.update.UpdateFragment"
        android:label="Update"
        tools:layout="@layout/fragment_update" >
        <action
            android:id="@+id/action_updateFragment_to_listFragment"
            app:destination="@id/listFragment" />
        <argument
            android:name="currentSantri"
            app:argType="id.ac.poltektegal.uasmpdanoop2.model.Santri" />
    </fragment>


    <fragment
        android:id="@+id/listFragmentAsatid"
        android:name="id.ac.poltektegal.uasmpdanoop2.fragments.list.ListFragmentAsatid"
        android:label="List"
        tools:layout="@layout/fragment_list_asatid" >
        <action
            android:id="@+id/action_listFragmentAsatid_to_addFragmentAsatid"
            app:destination="@id/addFragmentAsatid" />
        <action
            android:id="@+id/action_listFragmentAsatid_to_updateFragmentAsatid"
            app:destination="@id/updateFragmentAsatid" />
        <action
            android:id="@+id/action_listFragmentAsatid_to_home2"
            app:destination="@id/home2" />
    </fragment>


    <fragment
        android:id="@+id/addFragmentAsatid"
        android:name="id.ac.poltektegal.uasmpdanoop2.fragments.add.addFragmentAsatid"
        android:label="Add"
        tools:layout="@layout/fragment_add_asatid" >
        <action
            android:id="@+id/action_addFragmentAsatid_to_listFragmentAsatid"
            app:destination="@id/listFragmentAsatid" />
    </fragment>


    <fragment
        android:id="@+id/updateFragmentAsatid"
        android:name="id.ac.poltektegal.uasmpdanoop2.fragments.update.UpdateFragmentAsatid"
        android:label="Update"
        tools:layout="@layout/fragment_update_asatid" >
        <action
            android:id="@+id/action_updateFragmentAsatid_to_listFragmentAsatid"
            app:destination="@id/listFragmentAsatid" />
        <argument
            android:name="currentAsatid"
            app:argType="id.ac.poltektegal.uasmpdanoop2.model.Asatid" />
    </fragment>
    <fragment
        android:id="@+id/home2"
        android:name="id.ac.poltektegal.uasmpdanoop2.Home"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_home2_to_listFragment"
            app:destination="@id/listFragment" />
        <action
            android:id="@+id/action_home2_to_listFragmentAsatid"
            app:destination="@id/listFragmentAsatid" />
    </fragment>
</navigation>